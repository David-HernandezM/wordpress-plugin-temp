{
  "name": "vara-wordpress",
  "version": "1.0.0",
  "scripts": {
    "build": "wp-scripts build && pnpm run build:sailscalls && node build.js",
    "build:sailscalls": "webpack --config webpack.sailscalls.config.js",
    "newBlock": "node newComponent.js",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "all-steps": "pnpm run clean && pnpm run build && pnpm run rm-wordpress && pnpm run cp-wordpress && echo '\ndone!!!'",
    "cbuild": "pnpm run clean && pnpm run build",
    "clean": "rm -rf build vara-wordpress-dist",
    "cp-wordpress": "sudo cp -r vara-wordpress-dist /var/www/html/wp-content/plugins/",
    "rm-wordpress": "sudo rm -rfv /var/www/html/wp-content/plugins/vara-wordpress-dist",
    "allw": "pnpm run cleanw && pnpm run build && pnpm run rm-wordpress-w && pnpm run cp-wordpress-w && echo \"done!!\"",
    "cbuildw": "pnpm run cleanw && pnpm run build",
    "cleanw": "rimraf vara-wordpress-dist",
    "rm-wordpress-w": "rimraf \"C:\\\\xampp\\\\htdocs\\\\wordpress\\\\wp-content\\\\plugins\\\\vara-wordpress-dist\"",
    "cp-wordpress-w": "cpy \"vara-wordpress-dist/**/*\" \"C:\\\\xampp\\\\htdocs\\\\wordpress\\\\wp-content\\\\plugins\\\\vara-wordpress-dist\" --cwd=. --parents"
  },
  "devDependencies": {
    "@polkadot/extension-inject": "^0.59.2",
    "@polkadot/types": "15.10.2",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@types/wordpress__block-editor": "^11.5.16",
    "@types/wordpress__blocks": "^12.5.17",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@wordpress/scripts": "^26.19.0",
    "cpy-cli": "^5.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.28.0",
    "process": "^0.11.10",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "types": "link:@polkadot/extension-inject/types",
    "typescript": "^5.8.3",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@gear-js/api": "^0.41.1",
    "@gear-js/ui": "^0.7.0",
    "@gear-js/vara-ui": "^0.3.1",
    "@polkadot/api": "^15.10.2",
